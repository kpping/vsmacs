[
  {
    "key": "backspace",
    "command": "vsmacs.deleteLeft",
    "when": "isMarkMode && textInputFocus"
  },
  {
    "key": "shift+backspace",
    "command": "vsmacs.deleteLeft",
    "when": "isMarkMode && textInputFocus"
  },
  {
    "key": "ctrl+d",
    "command": "deleteRight",
    "when": "textInputFocus"
  },
  {
    "key": "delete",
    "command": "vsmacs.deleteRight",
    "when": "isMarkMode && textInputFocus"
  },
  {
    "key": "ctrl+d",
    "command": "vsmacs.deleteRight",
    "when": "isMarkMode && textInputFocus"
  },
  {
    "mac": "cmd+x",
    "command": "vsmacs.editorActionClipboardCutAction",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "shift+delete",
    "command": "vsmacs.editorActionClipboardCutAction",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+w",
    "command": "",
    "when": "!editorIsOpen && !multipleEditorGroups"
  },
  {
    "key": "ctrl+w",
    "command": "",
    "when": "activeEditorGroupEmpty && multipleEditorGroups"
  },
  {
    "key": "ctrl+w",
    "command": "vsmacs.editorActionClipboardCutAction",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+c",
    "mac": "cmd+c",
    "command": "vsmacs.editorActionClipboardCopyAction",
    "when": "textInputFocus"
  },
  {
    "key": "alt+w",
    "command": "",
    "when": "terminalFindFocused || terminalFocus"
  },
  {
    "key": "alt+w",
    "command": "",
    "when": "editorFocus"
  },
  {
    "key": "alt+w",
    "command": "",
    "when": "inSearchEditor && searchInputBoxFocus"
  },
  {
    "key": "alt+w",
    "command": "",
    "when": "searchViewletFocus"
  },
  {
    "key": "alt+w",
    "command": "vsmacs.editorActionClipboardCopyAction",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+v",
    "mac": "cmd+v",
    "command": "vsmacs.editorActionClipboardPasteAction",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+y",
    "command": "vsmacs.editorActionClipboardPasteAction",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+k",
    "command": "vsmacs.cutAllRight",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+j",
    "command": ""
  },
  {
    "key": "ctrl+j",
    "command": "vsmacs.editorActionInsertLineAfter",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+backspace",
    "command": "vsmacs.deleteWordLeft",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+delete",
    "command": "vsmacs.deleteWordRight",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "alt+d",
    "command": "vsmacs.deleteWordRight",
    "when": "textInputFocus && !editorReadonly"
  }
]
