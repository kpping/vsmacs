[
  {
    "key": "ctrl+x ctrl+f",
    "command": "workbench.action.files.openFile"
  },
  {
    "key": "ctrl+x ctrl+f",
    "command": "workbench.action.files.openLocalFile",
    "when": "remoteFileDialogVisible"
  },
  {
    "key": "ctrl+x ctrl+s",
    "command": "workbench.action.files.save"
  },
  {
    "key": "ctrl+x s",
    "command": "workbench.action.files.saveAll"
  }
]
