{
    "name": "vsmacs",
    "displayName": "VSmacs",
    "description": "Emacs Key Bindings for Visual Studio Code. No Kill Ring. No Fancy Things.",
    "version": "0.0.5",
    "license": "MIT",
    "publisher": "kpping",
    "engines": {
        "vscode": "^1.18.1"
    },
    "categories": [
        "Keymaps"
    ],
    "keywords": [
        "keymaps",
        "emacs",
        "key bindings"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [],
        "keybindings": [
            {
                "key": "alt+x",
                "command": "workbench.action.showCommands"
            },
            {
                "key": "ctrl+f",
                "command": "vsmacs.MoveCursorRight",
                "when": "editorTextFocus"
            },
            {
                "key": "right",
                "command": "vsmacs.MoveCursorRight",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+n",
                "command": "vsmacs.MoveCursorDown",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+n",
                "command": "workbench.action.quickOpenNavigateNext",
                "when": "inQuickOpen"
            },
            {
                "key": "ctrl+n",
                "command": "selectNextSuggestion",
                "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
            },
            {
                "key": "ctrl+n",
                "command": "showNextParameterHint",
                "when": "editorTextFocus && parameterHintsMultipleSignatures && parameterHintsVisible"
            },
            {
                "key": "ctrl+n",
                "command": "repl.action.historyNext",
                "when": "editorTextFocus && inDebugRepl && onLastDebugReplLine"
            },
            {
                "key": "down",
                "command": "vsmacs.MoveCursorDown",
                "when": "editorTextFocus"
            },
            {
                "key": "down",
                "command": "workbench.action.quickOpenNavigateNext",
                "when": "inQuickOpen"
            },
            {
                "key": "down",
                "command": "selectNextSuggestion",
                "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
            },
            {
                "key": "down",
                "command": "showNextParameterHint",
                "when": "editorTextFocus && parameterHintsMultipleSignatures && parameterHintsVisible"
            },
            {
                "key": "down",
                "command": "repl.action.historyNext",
                "when": "editorTextFocus && inDebugRepl && onLastDebugReplLine"
            },
            {
                "key": "ctrl+p",
                "command": "vsmacs.MoveCursorUp",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+p",
                "command": "workbench.action.quickOpenNavigatePrevious",
                "when": "inQuickOpen"
            },
            {
                "key": "ctrl+p",
                "command": "search.focus.nextInputBox",
                "when": "inputBoxFocus && searchViewletVisible"
            },
            {
                "key": "ctrl+p",
                "command": "selectPrevSuggestion",
                "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
            },
            {
                "key": "ctrl+p",
                "command": "showPrevParameterHint",
                "when": "editorTextFocus && parameterHintsMultipleSignatures && parameterHintsVisible"
            },
            {
                "key": "ctrl+p",
                "command": "repl.action.historyPrevious",
                "when": "editorTextFocus && inDebugRepl && onFirsteDebugReplLine"
            },
            {
                "key": "up",
                "command": "vsmacs.MoveCursorUp",
                "when": "editorTextFocus"
            },
            {
                "key": "up",
                "command": "workbench.action.quickOpenNavigatePrevious",
                "when": "inQuickOpen"
            },
            {
                "key": "up",
                "command": "search.focus.nextInputBox",
                "when": "inputBoxFocus && searchViewletVisible"
            },
            {
                "key": "up",
                "command": "selectPrevSuggestion",
                "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
            },
            {
                "key": "up",
                "command": "showPrevParameterHint",
                "when": "editorTextFocus && parameterHintsMultipleSignatures && parameterHintsVisible"
            },
            {
                "key": "up",
                "command": "repl.action.historyPrevious",
                "when": "editorTextFocus && inDebugRepl && onFirsteDebugReplLine"
            },
            {
                "key": "ctrl+b",
                "command": "vsmacs.MoveCursorLeft",
                "when": "editorTextFocus"
            },
            {
                "key": "left",
                "command": "vsmacs.MoveCursorLeft",
                "when": "editorTextFocus"
            },
            {
                "key": "escape shift+,",
                "command": "vsmacs.MoveCursorTop",
                "when": "editorTextFocus"
            },
            {
                "key": "escape shift+.",
                "command": "vsmacs.MoveCursorBottom",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+e",
                "command": "vsmacs.MoveCursorEnd",
                "when": "editorTextFocus"
            },
            {
                "key": "end",
                "command": "vsmacs.MoveCursorEnd",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+a",
                "command": "vsmacs.MoveCursorHome",
                "when": "editorTextFocus"
            },
            {
                "key": "home",
                "command": "vsmacs.MoveCursorHome",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+w",
                "command": "vsmacs.Copy",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+w",
                "command": "vsmacs.Cut",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "key": "ctrl+y",
                "command": "vsmacs.Paste",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "key": "ctrl+k",
                "command": "vsmacs.Kill",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "key": "backspace",
                "command": "vsmacs.Backspace",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "key": "ctrl+space",
                "command": "vsmacs.StartMarkMode",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+g",
                "command": "vsmacs.StopMarkMode",
                "when": "editorTextFocus"
            },
            {
                "key": "escape escape",
                "command": "vsmacs.StopMarkMode",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+s",
                "command": "actions.find"
            },
            {
                "key": "ctrl+s",
                "command": "editor.action.nextMatchFindAction",
                "when": "findWidgetVisible"
            },
            {
                "key": "enter",
                "command": "vsmacs.PreviousMatchFindAction_CloseFindWidget",
                "when": "findWidgetVisible"
            },
            {
                "key": "ctrl+g",
                "command": "closeFindWidget",
                "when": "findWidgetVisible"
            },
            {
                "key": "escape escape",
                "command": "closeFindWidget",
                "when": "findWidgetVisible"
            },
            {
                "key": "ctrl+x ctrl+s",
                "command": "workbench.action.files.save"
            },
            {
                "key": "ctrl+x ctrl+w",
                "command": "workbench.action.files.saveAs"
            },
            {
                "key": "ctrl+x ctrl+f",
                "command": "workbench.action.quickOpen"
            },
            {
                "key": "ctrl+x ctrl+c",
                "command": "workbench.action.closeActiveEditor"
            },
            {
                "key": "ctrl+/",
                "command": "undo"
            },
            {
                "key": "ctrl+'",
                "command": "editor.action.triggerSuggest"
            },
            {
                "key": "ctrl+;",
                "command": "editor.action.triggerParameterHints"
            },
            {
                "key": "ctrl+x 3",
                "command": "workbench.action.splitEditor"
            },
            {
                "key": "ctrl+x 0",
                "command": "workbench.action.closeEditorsInGroup"
            },
            {
                "key": "ctrl+x 1",
                "command": "workbench.action.closeOtherEditors"
            },
            {
                "key": "ctrl+x o",
                "command": "workbench.action.navigateEditorGroups"
            },
            {
                "mac": "cmd+1",
                "key": "alt+1",
                "command": "workbench.action.openEditorAtIndex1"
            },
            {
                "mac": "cmd+2",
                "key": "alt+2",
                "command": "workbench.action.openEditorAtIndex2"
            },
            {
                "mac": "cmd+3",
                "key": "alt+3",
                "command": "workbench.action.openEditorAtIndex3"
            },
            {
                "mac": "cmd+4",
                "key": "alt+4",
                "command": "workbench.action.openEditorAtIndex4"
            },
            {
                "mac": "cmd+5",
                "key": "alt+5",
                "command": "workbench.action.openEditorAtIndex5"
            },
            {
                "mac": "cmd+6",
                "key": "alt+6",
                "command": "workbench.action.openEditorAtIndex6"
            },
            {
                "mac": "cmd+7",
                "key": "alt+7",
                "command": "workbench.action.openEditorAtIndex7"
            },
            {
                "mac": "cmd+8",
                "key": "alt+8",
                "command": "workbench.action.openEditorAtIndex8"
            },
            {
                "mac": "cmd+9",
                "key": "alt+9",
                "command": "workbench.action.openEditorAtIndex9"
            },
            {
                "key": "ctrl+g",
                "command": "workbench.action.exitZenMode",
                "when": "inZenMode"
            },
            {
                "key": "ctrl+g",
                "command": "closeReferenceSearchEditor",
                "when": "inReferenceSearchEditor && !config.editor.stablePeek"
            },
            {
                "key": "ctrl+g",
                "command": "removeSecondaryCursors",
                "when": "editorHasMultipleSelections && editorTextFocus"
            },
            {
                "key": "ctrl+g",
                "command": "closeBreakpointWidget",
                "when": "breakpointWidgetVisible && editorFocus"
            },
            {
                "key": "ctrl+g",
                "command": "leaveSnippet",
                "when": "editorTextFocus && inSnippetMode"
            },
            {
                "key": "ctrl+g",
                "command": "closeMarkersNavigation",
                "when": "editorFocus && markersNavigationVisible"
            },
            {
                "key": "ctrl+g",
                "command": "closeReferenceSearch",
                "when": "referenceSearchVisible && !config.editor.stablePeek"
            },
            {
                "key": "ctrl+g",
                "command": "closeParameterHints",
                "when": "editorTextFocus && parameterHintsVisible"
            },
            {
                "key": "ctrl+g",
                "command": "hideSuggestWidget",
                "when": "editorTextFocus && suggestWidgetVisible"
            },
            {
                "key": "ctrl+g",
                "command": "cancelRenameInput",
                "when": "editorFocus && renameInputVisible"
            },
            {
                "key": "ctrl+g",
                "command": "closeAccessibilityHelp",
                "when": "accessibilityHelpWidgetVisible && editorFocus"
            },
            {
                "key": "ctrl+g",
                "command": "closeReplaceInFilesWidget",
                "when": "replaceInputBoxFocus && searchViewletVisible"
            },
            {
                "key": "ctrl+g",
                "command": "workbench.action.closeMessages",
                "when": "globalMessageVisible"
            },
            {
                "key": "ctrl+g",
                "command": "workbench.action.closeQuickOpen",
                "when": "inQuickOpen"
            },
            {
                "key": "ctrl+g",
                "command": "workbench.action.hideInterfaceOverview",
                "when": "interfaceOverviewVisible"
            },
            {
                "key": "ctrl+j",
                "command": "editor.action.insertLineAfter",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "key": "escape escape",
                "command": "workbench.action.exitZenMode",
                "when": "inZenMode"
            },
            {
                "key": "escape escape",
                "command": "closeReferenceSearchEditor",
                "when": "inReferenceSearchEditor && !config.editor.stablePeek"
            },
            {
                "key": "escape escape",
                "command": "removeSecondaryCursors",
                "when": "editorHasMultipleSelections && editorTextFocus"
            },
            {
                "key": "escape escape",
                "command": "closeBreakpointWidget",
                "when": "breakpointWidgetVisible && editorFocus"
            },
            {
                "key": "escape escape",
                "command": "leaveSnippet",
                "when": "editorTextFocus && inSnippetMode"
            },
            {
                "key": "escape escape",
                "command": "closeMarkersNavigation",
                "when": "editorFocus && markersNavigationVisible"
            },
            {
                "key": "escape escape",
                "command": "closeReferenceSearch",
                "when": "referenceSearchVisible && !config.editor.stablePeek"
            },
            {
                "key": "escape escape",
                "command": "closeParameterHints",
                "when": "editorTextFocus && parameterHintsVisible"
            },
            {
                "key": "escape escape",
                "command": "hideSuggestWidget",
                "when": "editorTextFocus && suggestWidgetVisible"
            },
            {
                "key": "escape escape",
                "command": "cancelRenameInput",
                "when": "editorFocus && renameInputVisible"
            },
            {
                "key": "escape escape",
                "command": "closeAccessibilityHelp",
                "when": "accessibilityHelpWidgetVisible && editorFocus"
            },
            {
                "key": "escape escape",
                "command": "closeReplaceInFilesWidget",
                "when": "replaceInputBoxFocus && searchViewletVisible"
            },
            {
                "key": "escape escape",
                "command": "workbench.action.closeMessages",
                "when": "globalMessageVisible"
            },
            {
                "key": "escape escape",
                "command": "workbench.action.closeQuickOpen",
                "when": "inQuickOpen"
            },
            {
                "key": "escape escape",
                "command": "workbench.action.hideInterfaceOverview",
                "when": "interfaceOverviewVisible"
            }
        ]
    },
    "scripts": {
    "tsc:onlyts": "tsc -b tsconfig-onlyts.json",
    "tsc:spects": "tsc -b tsconfig-spects.json",
    "compile": "yarn tsc:onlyts && yarn tsc:spects",
    "watch": "tsc -b tsconfig-onlyts.json --watch",
    "lint": "eslint --ext .ts ./src --fix",
    "prettier:config": "prettier --no-config --write \"./**/*.{json,yml}\"",
    "prettier:ts": "prettier --write \"./**/*.ts\"",
    "prettier": "yarn prettier:config --loglevel warn && yarn prettier:ts --loglevel warn",
    "test": "jest --passWithNoTests",
    "build": "yarn prettier && yarn lint && yarn compile && yarn test"
  },
  "dependencies": {
    "@ag1/nil": "^1.1.0"
    },
    "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^10",
    "@types/vscode": "^1.38.0",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2"
    },
    "homepage": "https://github.com/kpping/vsmacs/blob/master/README.md",
    "bugs": "https://github.com/kpping/vsmacs/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/kpping/vsmacs.git"
    }
}
